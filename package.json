{
  "name": "wiki-circuit-client",
  "version": "2.0.0",
  "description": "The web browser client component of the Wiki Circuit project",
  "license": "MIT",
  "author": {
    "name": "Harlan Sang",
    "email": "harlan@linkedmink.space"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LinkedMink/wiki-circuit-client"
  },
  "private": true,
  "scripts": {
    "start": "webpack serve --config webpack.local.js",
    "build": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "build:serve": "tsc --project ./tsconfig.serve.json",
    "build:serve:prod": "tsc --project ./tsconfig.prod.json",
    "test": "jest",
    "lint": "eslint -c .eslintrc.js src/**/*.[tj]s{,x}",
    "format": "prettier --write src/**/*.[tj]s{,x} tests/**/*.[tj]s serve/**/*.[tj]s *.js{,on}",
    "startProduction": "node ./dist/server.js",
    "containerize": "npm run build && docker build -t linkedmink/wiki-circuit-client ."
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "jsonwebtoken": "^8.5.1",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1",
    "query-string": "^6.13.8",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.3",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@types/express": "^4.17.11",
    "@types/express-serve-static-core": "^4.17.17",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-transition-group": "^4.4.0",
    "@types/url-join": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.4.3",
    "file-loader": "^6.2.0",
    "jest": "^26.6.3",
    "node-sass": "^4.14.1",
    "prettier": "^2.2.1",
    "react-hot-loader": "^4.13.0",
    "sass-loader": "^10.1.1",
    "source-map-loader": "^1.1.3",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
}
